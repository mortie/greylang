#!/usr/bin/env grey

// Simple object
obj = #{
	foo: "bar",
	baz: 100
};

print(obj.foo); // Prints "bar"

// Functions in objects have a 'self' variable
obj2 = #{
	foo: "bar",
	func: {
		print(self.foo);
	}
};

obj2.func(); // Prints "bar"

// We can modify objects:
obj2.foo = "baz";
obj2.func() // Prints "baz"
