Range := {
	// Constructor takes a min (inclusive) and max (exclusive)
	$init: {(min, max)
		self.min = min;
		self.max = max;
	},

	$iter: {
		i := self.min;

		next := {
			if(i < self.max, {
				i = i + 1;
				i - 1;
			}, { none });
		};

		{ $next: next };
	}
};

// x percent of y
%of := {(x, y)
	y * (x / 100)
};
