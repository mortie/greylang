// Iterator between min (inclusive) and max (exclusive)
range = {(min, max)
	RangeIter = {
		next: {
			self.ret = none;
			if (self.i < self.max, {
				self.ret = self.i;
				self.i = self.i + 1;
			});
			self.ret
		},

		init: {
			self.i = self.min;
			self
		}
	};

	{
		iter: {
			new(RangeIter, { min: min, max: max }).init();
		}
	}
};

// x percent of y
%of = {(x, y)
	y * (x / 100)
};
